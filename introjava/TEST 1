import java.util.*;
public class Main
 {
	
	public static void main(String[] args)
	 {
		// Write your code here
		Scanner T = new Scanner(System.in);
		int n = T.nextInt();
		int array[] = new int[n];
		for (int i = 0; i < n; i++) {
			array[i] = T.nextInt();			
		}
		int d = array[1] - array[0];
		boolean Result = true;
		for (int i = 1; i < array.length; i++)
		 {
			if (array[i]-array[i-1]!=d)
			{
				Result = false;
				break;
			}			
		}
		System.out.println(Result); // Result :- Line 1 as total term & line 2 as given no

	}
}

output:

6
2 6 10 15 19 23 < is ap or not???>
false

