/*******************************************Number Star pattern 1**********   ***************************************************/
*********************************************************************************************************************************
import java.util.*;
public class runner
 {

	public static void main(String[] args) 
	{
		// Write your code here
		Scanner A = new Scanner(System.in);
		int n = A.nextInt();
		for(int i = 1 ; i<=n; i ++ )
		{
			for (int j = n; j>=1; j --)
			{
				if (j ==i)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(j);
				}
			}
			System.out.println();
		}

	}
}

****************************************************************************************************************************
output:::
6
result::
65432*
6543*1
654*21
65*321
6*4321
*54321
