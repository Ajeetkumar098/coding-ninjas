public static void main(String[] args) {
        int a = 10, b = 20, c = 100;

        if(a <= b && b != 20) {
            System.out.println("hello");
        }
        else if(c >= a && c >= b) {
            System.out.println("hi");
        }
        else {
            System.out.println("hey");
        }
}

